{
  "store.search": {
    "blocks": [
      "search-result-layout"

    ],
    "props": {
      "context": {
        "orderByField": "OrderByScoreDESC",
        "skusFilter": "FIRST_AVAILABLE",
        "facetsBehavior": "dynamic",
        "simulationBehavior": "skip"
      }
    }
  },



  "store.search#brand": {
    "blocks": [
      "search-result-layout"
    ],
    "props": {
      "context": {
        "orderByField": "OrderByScoreDESC",
        "maxItemsPerPage": 10,
        "skusFilter": "FIRST_AVAILABLE",
        "facetsBehavior": "dynamic",
        "simulationBehavior": "skip"
      }
    }
  },
  "store.search#department": {
    "blocks": [
      "search-result-layout"
    ],
    "props": {
      "context": {
        "skusFilter": "FIRST_AVAILABLE",
        "facetsBehavior": "dynamic",
        "simulationBehavior": "skip"
      }
    }
  },
  "store.search#category": {
    "blocks": [
      "search-result-layout"
    ],
    "props": {
      "context": {
        "skusFilter": "FIRST_AVAILABLE",
        "facetsBehavior": "dynamic",
        "simulationBehavior": "skip"
      }
    }
  },
  "store.search#subcategory": {
    "blocks": [
      "search-result-layout"
    ],
    "props": {
      "context": {
        "skusFilter": "FIRST_AVAILABLE",
        "facetsBehavior": "dynamic",
        "simulationBehavior": "skip"
      }
    }
  },
  "search-result-layout": {
    "blocks": [
      "search-result-layout.desktop.biscoind",
      "search-result-layout.mobile.biscoind",
      "search-not-found-layout"


    ]
  },
  "search-result-layout.desktop.biscoind": {
    "children": [
      "flex-layout.row#did-you-mean",
      "flex-layout.row#suggestion",
      "flex-layout.row#searchbread",
      "flex-layout.row#banner-one",
      "flex-layout.row#result"
    ],
    "props": {
      "pagination": "show-more",
      "showFacetQuantity": true,
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      }
    }
  },
  "flex-layout.row#banner-one": {
    "children": [
      "search-banner#one"
    ],
    "props": {
      "blockClass": "myBannereeee"
    }
  },
  "search-banner#one": {
    "props": {
      "area": "one",
      "blockClass": "myBanner",
      "horizontalAlignment": "center"
    }
  },
  "flex-layout.row#did-you-mean": {
    "children": [
      "did-you-mean"
    ]
  },
  "flex-layout.row#suggestion": {
    "children": [
      "search-suggestions"
    ]
  },
  "flex-layout.row#searchbread": {
    "children": [
      "breadcrumb.search"
    ],
    "props": {
      "preserveLayoutOnMobile": false,
      "fullWidth": true,
      "blockClass": "search-breadcrumb"
    }
  },
  "flex-layout.row#result": {
    "children": [
      "flex-layout.col#filter",
      "flex-layout.col#content"
    ],
    "props": {
      "preventHorizontalStretch": false,
      "fullWidth": true,
      "blockClass": "search-result-layout-desktop"
    }
  },
  "filter-navigator.v3":{
    "props":{
      "blockClass":"search-result-facets"
    }
  },
  "flex-layout.col#filter": {
    "children": [
      "search-title.v2",
      "filter-navigator.v3"
    ],
    "props": {
      "blockClass": "filterCol",
      "preventVerticalStretch": true,
      "width": "20%"
    }
  },
  "flex-layout.col#content": {
    "children": [
      "brand-content.search",
      "category-gallery.search",
      "flex-layout.row#searchinfo",
      "flex-layout.row#fetchprevious",
      "flex-layout.row#products",
      "flex-layout.row#fetchmore"
    ],
    "props": {
      "width": "80%",
      "blockClass": "search-result-container",
      "preventVerticalStretch": true
    }
  },
  "flex-layout.row#searchinfo": {
    "children": [
      "flex-layout.col#productCount",
      "flex-layout.col#orderby"
    ],
    "props": {
      "blockClass": "search-info-row"
    }
  },
  "flex-layout.col#productCount": {
    "children": [
      "total-products.v2"
    ],
    "props": {
      "blockClass": "productCountCol"
    }
  },
  "flex-layout.col#orderby": {
    "children": [
      "order-by.v2"
    ],
    "props": {
      "blockClass": "orderByCol"
    }
  },
  "order-by.v2": {
    "props": {
      "hiddenOptions": [
        "OrderByTopSaleDESC",
        "OrderByBestDiscountDESC",
        "OrderByReleaseDateDESC"
      ]
    }
  },
  "flex-layout.row#fetchprevious": {
    "props": {
      "marginBottom": 3
    },
    "children": [
      "search-fetch-previous"
    ]
  },
  "flex-layout.row#fetchmore": {
    "props": {
      "marginTop": 3
    },
    "children": [
      "search-fetch-more"
    ]
  },
  "flex-layout.row#products": {
    "children": [
      "search-content#desktop"
    ]
  },
  "search-content#mobile": {
    "blocks": [
      "gallery#mobile",
      "not-found"
    ]
  },
  "search-content#desktop": {
    "blocks": [
      "gallery#desktop",
      "not-found"
    ]
  },
  "search-result-layout.mobile.biscoind": {
    "children": [
      "flex-layout.row#did-you-mean",
      "flex-layout.row#suggestion",
      "flex-layout.row#banner-one",
      "category-gallery.search",
      "flex-layout.row#searchinfomobile",
      "__fold__",
      "__fold__.experimentalLazyAssets",
      "flex-layout.row#productCountMobile",
      "flex-layout.row#contentmobile"
    ],
    "props": {
      "pagination": "show-more",
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      }
    }
  },
  "flex-layout.row#contentmobile": {
    "children": [
      "search-content#mobile"
    ],
    "props": {
      "preserveLayoutOnMobile": true
    }
  },
  "flex-layout.row#searchinfomobile": {
    "children": [
      "flex-layout.col#orderByMobile",
      "flex-layout.col#filterMobile",
      "flex-layout.col#switcherMobile"
    ],
    "props": {
      "preserveLayoutOnMobile": true,
      "colSizing": "auto",
      "colJustify": "around"
    }
  },
  "flex-layout.col#orderByMobile": {
    "children": [
      "order-by.v2"
    ],
    "props": {
      "blockClass": "orderByMobileCol"
    }
  },
  "flex-layout.row#productCountMobile": {
    "children": [
      "total-products.v2"
    ],
    "props": {
      "blockClass": "productCountMobileRow"
    }
  },
  "flex-layout.col#filterMobile": {
    "children": [
      "filter-navigator.v3"
    ],
    "props": {
      "blockClass": "filterMobileCol"
    }
  },
  "flex-layout.col#switcherMobile": {
    "children": [
      "search-layout-switcher"
    ],
    "props": {
      "blockClass": "switcherMobileCol"
    }
  },
  "rich-text#trysomebestdiscountproducts":{
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "**Try our some best discount products**",
      "textColor": "c-on-emphasis",
      "font": "t-heading-5",
      "blockClass":"discountclass"
    }
  },
  "search-not-found-layout": {
    "children": [
      "flex-layout.row#searchbread",
      "flex-layout.row#notfound"
    ]
  },

  "rich-text#error":{
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "This is custom error message na",
      "font": "t-heading-5",
      "blockClass": "help-message"
    }
  },
  "flex-layout.row#collection":{
    "children":["list-context.product-list#collections"],
    "props":{
      "blockClass":"search-result-collections"
    }
  },
  "list-context.product-list#collections":{
    "blocks": ["product-summary.shelf#collections"],
    "children": ["slider-layout#shelf"],
    "props": {
      "orderBy": "OrderByTopSaleDESC",
      "collection": "318",
      "paginationDotsVisibility": "desktopOnly"
    }
  },
  "product-summary.shelf#collections":{
    "children":[
      "product-summary-image",
      "product-summary-name",
      "product-rating-inline",
      "product-summary-space",
      "product-summary-price",
      "product-summary-buy-button"
    ]
  },
  "slider-layout#shelf": {
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 2,
        "phone": 2
      }
    }
  },
  "flex-layout.row#notfound": {
    "children": [
      "flex-layout.col#collection"
    ],
    "props": {
      "fullWidth": true
    }
  },
